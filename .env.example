# Waystt Environment Configuration Example
# Copy this file to .env and fill in your values

# OpenAI API Configuration (required for transcription)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Audio Configuration (optional - defaults shown)
AUDIO_BUFFER_DURATION_SECONDS=300
AUDIO_SAMPLE_RATE=16000
AUDIO_CHANNELS=1

# Transcription Configuration (optional)
WHISPER_MODEL=whisper-1
WHISPER_LANGUAGE=auto

# Logging Configuration (optional)
RUST_LOG=info